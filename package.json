  "main": "webapp/server.js",
  "scripts": {
    "build": "npm run clear && npm run webpack",
    "clear": "rm -fv ./webapp/public/build/* && rm -fv ./webapp/public/*.js && rm -fv ./webapp/public/*.map",
    "eslint": "eslint . ./src/js/**/*.{es6,jsx} ./test/**/*.es6 ./webapp/**/*.{es6,js}",
    "gremlins": "cp -f ./test/vendor/gremlins.min.js ./webapp/public/build/gremlins.min.js",
    "mocha": "mocha ./test/node/**/*.test.{es6,js} -t 10000 -R spec -u bdd --exit",
    "start": "forever --minUptime 5000 --spinSleepTime 10000 webapp/server.js",
    "test": "npm run mocha && npm run wdio",
    "upgrade": "ncu -u -x chalk,helmet && npm i",
    "uglify": "ver",
    "wdio": "npm run gremlins && wdio run ./wdio.conf.js",
    "webpack": "webpack --config ./webpack.config.js"
  },
